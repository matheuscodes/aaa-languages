{
  "appBarTitle":"Seasons Management",
  "newSeason":{
    "title":"New Season",
    "subtitle":"Please fill in the information below to create a new season.",
    "button":"New Season",
    "startDateDatepicker":{
      "label":"Season start"
    },
    "endDateDatepicker":{
      "label":"Season end"
    },
    "nameTextField":{
      "label":"Season name",
      "hint":"Type in the name of the season"
    },
    "equipmentSelectField":{
      "undefined":"No equipment",
      "label":"Equipment"
    },
    "arrowCountTextField":{
      "label":"Total # wk {{week}}",
      "hint":"# of arrows"
    },
    "targetShareTextField":{
      "label":"Target # wk {{week}}",
      "hint":"# of arrows"
    },
    "events":{
      "subheader":"Events",
      "noEvents":"No events found on the given dates.",
      "primaryText":"{{name}} ({{nameShort}})",
      "secondaryText":"{{date,dateLong}}"
    },
    "trainers":{
      "subheader":"Trainer Permissions",
      "noTrainers":"No trainers.",
      "primaryText":"Allow {{trainerName}}"
    }
  },

  "messages":{
    "newSaved":"New season was saved!",
    "newError":"New season could not be saved!",
    "listError":"Season could not be loaded!",
    "deleted":"Season was deleted!",
    "deletedError":"Season could not be deleted!",
    "registerSuccess":"Registration status was changed!",
    "registerError":"Registration status could not be changed!",
    "permissionSuccess":"Permission was changed!",
    "permissionError":"Permission could not be changed!"
  },

  "graphLabels":{
    "data":{
      "totalPlan":"Total Plan",
      "techniqueTotals":"Technique Totals",
      "techniqueShare":"Technique Goal",
      "targetTotals":"Target Totals",
      "resultTotals":"Result Totals",
      "strengthTraining":"Strength Training"
    }
  },
  "tile":{
    "subtitle":"{{start,dateLong}} to {{end,dateLong}}"
  }
}
